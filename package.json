{
  "name": "WickidoolSortingExample",
  "version": "1.0.0",
  "description": "Table sorting using ReactJS",
  "main": "app/index.js",
  "scripts": {
    "rebuild": "rm -rf node_modules && install",
    "test": "karma start conf/karma.conf.js --single-run",
    "test:all": "npm run test:client && npm run test:server",
    "test:client": "mocha 'src/client/js/**/__specs__/*.js' --compilers js:babel-core/register,css:./test/css-modules-compiler.js --require ./test/env_setup.js",
    "test:server": "_mocha --opts ./test/mocha.server.opts ",
    "test:watch": "karma start conf/karma.conf.js",
    "test:coverage": "karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "karma start conf/karma.ci.conf.js --single-run",
    "start": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "dev": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "prod": "node startup.js",
    "build": "webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist",
    "update": "ncu -u",
    "ncu": "ncu"
  },
  "author": "Allan Wick (al@wickidcool.com)",
  "license": "MIT",
  "dependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-redux": "^4.4.5",
    "redux": "^3.0.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.2.0",
    "react-fontawesome": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "chai":"3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "express": "4.14.0",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "isparta": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.5.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "mocha": "3.0.2",
    "node-sass": "^3.4.2",
    "npm-check-updates": "2.8.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-mock-store": "1.2.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "serve-static": "1.11.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.0",
    "webpack-dev-server": "^1.9.0"
  }
}
