    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.1.0",
    "react-fontawesome": "^0.3.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.0.0",
    "babel-eslint": "^4.0.0",
    "babel-loader": "^5.0.0",
