{
  "name": "WickidoolSortingExample",
  "version": "1.0.0",
  "description": "Table sorting using ReactJS",
  "main": "app/index.js",
  "scripts": {
    "rebuild": "rm -rf node_modules && install",
    "test": "karma start ./webpack/karma.config.js --single-run",
    "test:all": "npm run test:client && npm run test:server",
    "test:client": "mocha 'src/client/js/**/__specs__/*.js' --compilers js:babel-core/register,css:./test/css-modules-compiler.js --require ./test/env_setup.js",
    "test:server": "_mocha --opts ./test/mocha.server.opts ",
    "test:watch": "karma start conf/karma.conf.js",
    "test:coverage": "karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "karma start conf/karma.ci.conf.js --single-run",
    "start": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "prod": "webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "dev": "NODE_ENV=development BABEL_ENV=development babel-node ./webpack/webpack-dev-server.js & NODE_ENV=development BABEL_ENV=development babel-node ./src/server",
    "dev2": "./node_modules/.bin/webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --content-base ./build",
    "build": "webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist",
    "update": "ncu -u",
    "ncu": "ncu"
  },
  "author": "Allan Wick (al@wickidcool.com)",
  "license": "MIT",
  "dependencies": {
    "config": "1.21.0",
    "express-cache-response-directive": "1.0.0",
    "html-tagged-literals": "1.0.2",
    "memwatch-next": "0.3.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-bootstrap": "0.30.3",
    "react-redux": "4.4.5",
    "react-fontawesome": "1.2.0",
    "redux": "3.0.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "2.1.0",
    "superagent": "2.2.0",
    "useragent": "2.1.9"
  },
